{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11
  },
  "plugins": ["prettier"],
  "rules": {
    "semi": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "function-paren-newline": 0,
    "no-else-return": 0
    

  }
}
